citHeader("To cite sqlr.[INSERT THE NAME OF THE PACKAGE] in publications use:")

citEntry(
  entry       = "Unpublished",
  title       = paste0("sqlr.[INSERT THE NAME OF THE PACKAGE]: ",
                       "an R package with the research compendium of the ",
                       "project - [INSERT THE NAME OF THE PROJECT]"),
  author      = personList(as.person("[INSERT THE FIRST AUTHOR NAME]"),
                           as.person("Daniel Vartanian"),
                           as.person("Ana Amelia Benedito-Silva"),
                           as.person("Mario Pedrazzoli")),
  year        = "2021",
  url         = paste0("https://gipsousp.github.io/",
                        "sqlr.[INSERT THE NAME OF THE PACKAGE]/"),
  textVersion = paste0("[INSERT THE FIRST AUTHOR NAME]. Vartanian D., ",
                       "Benedito-Silva, A. A., Pedrazzoli, M. (2021). ",
                       "sqlr.[INSERT THE NAME OF THE PACKAGE]: ",
                       "an R package with the research compendium of the ",
                       "project - [INSERT THE NAME OF THE PROJECT]. ",
                       "Retrieved from https://gipsousp.github.io/",
                       "sqlr.[INSERT THE NAME OF THE PACKAGE]/ ."),
  note        = "Lifecycle: experimental"
)
